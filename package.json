{
  "scripts": {
    "build": "npx tsc && npx copyfiles -u 1 public/* build/public",
    "start": "npx ts-node index.ts",
    "start:dev": "nodemon index.ts",
    "db:migrate:dev": "npx prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma --script > down.sql && npx prisma migrate dev",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "ts-node prisma/seeds/main.ts",
    "db:migrate:execute": "npx prisma db execute --file ./down.sql --schema prisma/schema.prisma",
    "db:migrate:reset": "npx prisma migrate reset"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "bcrypt": "^5.1.1",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "nodemon": "^3.1.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5.5.4",
    "zod": "^3.23.8"
  },
  "name": "node-deep",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@prisma/client": "^5.17.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.14.11",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "prisma": "^5.17.0"
  }
}
